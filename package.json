  "main": "index.js",
  "dependencies": {
    "babel-polyfill": "^6.26.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.0",
    "autoprefixer": "^9.4.2",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.4",
    "babel-preset-env": "^1.7.0",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.0.1",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9"
  },
  "scripts": {
    "watch:sass": "node-sass src/scss/main.scss dist/css/style.css -w",
    "devserver": "live-server",
    "start:sass": "npm-run-all --parallel devserver watch:sass",

    "compile:sass": "node-sass src/scss/main.scss dist/css/style.comp.css",
    "compress:css": "node-sass dist/css/style.comp.css dist/css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass compress:css",

    "dev": "webpack --mode development -w",
    "build": "webpack --mode production",
    "start": "npm-run-all --parallel devserver watch:sass dev"
  },
